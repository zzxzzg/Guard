<?xml version="1.0" encoding="utf-8"?><!--<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"--><!--xmlns:card_view="http://schemas.android.com/apk/res-auto"--><!--android:id="@+id/card_view"--><!--android:layout_width="fill_parent"--><!--android:layout_margin="3dp"--><!--android:layout_height="200dp"--><!--android:background="@color/white"--><!--card_view:cardUseCompatPadding="true"--><!--card_view:cardCornerRadius="1dp"--><!-->-->
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/card_view"
    android:layout_width="fill_parent"
    android:layout_height="150dp"
    android:background="#ffffff">

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="#ffffff"
        >


        <RelativeLayout

            android:id="@+id/item_page"
            android:layout_width="fill_parent"
            android:layout_height="150dp"
            >

            <ImageView
                android:layout_margin="5dp"
                android:id="@+id/image"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:scaleType="fitXY"
                />


        </RelativeLayout>

    </RelativeLayout>
</FrameLayout><!--</android.support.v7.widget.CardView>-->
